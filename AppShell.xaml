<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Nyaa_Streamer.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Nyaa_Streamer"
   
    Title="Nyaa_Streamer">


    <FlyoutItem Title="Home">
        <ShellContent ContentTemplate="{DataTemplate local:MainPage}" />
    </FlyoutItem>
    <FlyoutItem Title="Trending">
        <ShellContent ContentTemplate="{DataTemplate local:TrendingAnimePage}" />
    </FlyoutItem>
    <FlyoutItem Title="Favorites">
        <ShellContent ContentTemplate="{DataTemplate local:FavoriteAnimePage}" />
    </FlyoutItem>
    <FlyoutItem Title="Watchlist">
        <ShellContent ContentTemplate="{DataTemplate local:WatchlistPage}" />
    </FlyoutItem>
    <FlyoutItem Title="Search">
        <ShellContent ContentTemplate="{DataTemplate local:SearchAnimePage}" />
    </FlyoutItem>
</Shell>
